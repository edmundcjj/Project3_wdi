<% @users.each do |user| %>
	  <tr>
	    <td><%= user.name %></td>
	    <td><%= user.email %></td>
	    <td><%= user.admin %></td>
	    <td><%= user.last_sign_in_at %></td>
	    <td><%= user.created_at %></td>
	    <td><%= link_to "Edit", edit_admin_user_path(user.id),remote: true, class: "btn btn-default" %>
	    </td>
	  </tr>
<% end %>